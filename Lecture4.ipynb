   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "exam_avgs=[] \n",
    "k=0\n",
    "while k < len (class_grades):\n",
    "    avg = class_grades[k][0] + class_grades[k][1] + class_grades[k][2]\n",
